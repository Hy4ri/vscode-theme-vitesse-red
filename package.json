{
  "publisher": "M57",
  "name": "m57",
  "displayName": "M57",
  "version": "0.1.0",
  "packageManager": "pnpm@9.1.1",
  "description": "fork of Vitesse theme for VS Code",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hy4ri/vscode-theme-vitesse-red"
  },
  "bugs": {
    "url": "https://github.com/hy4ri/vscode-theme-vitesse-red/issues"
  },
  "keywords": [
    "theme",
    "vitesse",
    "light",
    "dark",
    "m57",
    "red"
  ],
  "categories": [
    "Themes"
  ],
  "exports": {
    "./*": "./*"
  },
  "icon": "icon.png",
  "files": [
    "extra",
    "themes"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "M57",
        "uiTheme": "vs-dark",
        "path": "./themes/m57.json"
      }
    ]
  },
  "scripts": {
    "build": "esno scripts/index.ts",
    "dev": "esno watch scripts/index.ts",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "release": "bumpp --all && vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.17.0",
    "@antfu/utils": "^0.7.10",
    "@types/color": "^3.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.11",
    "@vscode/vsce": "^2.26.1",
    "bumpp": "^9.4.1",
    "color": "^4.2.3",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "fs-extra": "^11.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "xterm": "^5.3.0"
  },
  "preview": true
}
